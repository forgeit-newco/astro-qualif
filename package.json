    "@mui/material": "^5.15.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.50.0",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
